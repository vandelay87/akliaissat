{
    "extends": [
        "airbnb-base",
        "plugin:vue/recommended",
        "prettier",
        "prettier/babel",
        "prettier/vue"
    ],
    "plugins": [
      "babel",
      "prettier",
      "vue"
    ],
    "env": {
        "browser": true,
        "es6": true,
        "node": true,
        "jest": true
    },
     "parserOptions": {
         "parser": "babel-eslint",
         "ecmaFeatures": {
           "vue": true
         }
     },
    "rules": {
      "prettier/prettier": "error"
    }
}
